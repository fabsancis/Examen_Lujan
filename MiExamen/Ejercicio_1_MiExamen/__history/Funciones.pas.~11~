unit Funciones;

interface

uses
Tipos, Sysutils,
StackPointer;
Function NoMultiplos(Var atP: Pila; N: integer): Pila;



implementation


Function NoMultiplos(Var atP: Pila; N: integer): Pila;
Var
X: TipoElemento;
nmPila: Pila;
AuxP: Pila;

Begin
nmPila.Crear(atP.DatoDeLaClave,atP.SizeStack);
AuxP.Crear(atP.DatoDeLaClave,atP.SizeStack);
if atP.EsVacia then
  result:= nmPila
else
  Begin
  X:= atP.Recuperar;
  atP.DesApilar;
  AuxP.Apilar(X);
  if X.Clave mod N <> 0 then
    Begin
      nmPila.Apilar(X);
    End;
  End;

NoMultiplos(atP,N);

End;

end.
